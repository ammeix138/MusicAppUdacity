<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ammei.musicappudacity">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="My Music"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MusicActivity" android:label="Music Library"/>
        <activity android:name=".ArtistActivity" android:label="Artist List"/>
        <activity android:name=".PlaylistActivity" android:label="My Playlist's"/>
        <activity android:name=".DownloadActivity" android:label="Download Music" />
        <activity android:label="Now Playing" android:name=".NowPlayingActivity">
        </activity>
    </application>

</manifest>